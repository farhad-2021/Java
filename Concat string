#include<stdio.h>
void main()
{
    char str1[100];
    char str2[100];
    int i=0,j=0;
    printf("Enter a string :");

    gets(str1);

    printf("Enter a string :");
    gets(str2);

    while(str1[i]!='\0')

        i++;


    while(str2[j]!= '\0')
    {

        str1[i]=str2[j];
        j++;
        i++;

    }

    str1[i]!='\0';
    printf("\n Concatenated String is %s\n",str1);


    printf("Length of the string : %d",i);

}
